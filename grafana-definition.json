{
    "family": "grafana-definition",
    "taskRoleArn": "arn:aws:iam::980136594430:role/ecsTaskExecutionRole",
        "containerDefinitions": [
            {
                "name": "grafana",
                "image": "grafana/grafana",
                "cpu": 256,
                "memory": 256,
                "memoryReservation": 128,
                "portMappings": [
                    {
                        "containerPort": 3000,
                        "hostPort": 3000,
                        "protocol": "tcp"
                    }
                ],
                "essential": true
            }
        ],

    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "256",
    "memory": "256"
}
